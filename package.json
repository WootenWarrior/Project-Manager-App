{
  "name": "project-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "/",
  "scripts": {
    "start": "npx tsx src/server/Server.ts",
    "dev": "cross-env NODE_ENV=dev vite --host --mode dev",
    "build": "tsc -b && vite build --mode prod",
    "run": "cross-env NODE_ENV=prod vite --host --mode prod",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:all": "concurrently \"npm:start\" \"npm:dev\"",
    "run:all": "concurrently \"npm:start\" \"npm:run\""
  },
  "dependencies": {
    "@atlaskit/pragmatic-drag-and-drop": "^1.5.0",
    "@dnd-kit/core": "^6.3.1",
    "@tailwindcss/vite": "^4.0.12",
    "@types/jsonwebtoken": "^9.0.7",
    "axios": "^1.8.4",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "firebase": "^11.0.2",
    "firebase-admin": "^13.2.0",
    "gsap": "^3.12.7",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "node": "^23.6.0",
    "nodemon": "^3.1.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^7.1.1",
    "tailwindcss": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}
